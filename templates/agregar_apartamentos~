			function validar(){  
				if(jQuery('#txtRestaurante').val().length < 1) {   
					jQuery( "#mensaje2" ).dialog(); 
				}
				else if(jQuery("#txtPlatillo").val().length < 1){
					jQuery( "#mensaje2" ).dialog(); 
				}
				else if(jQuery("#ingredientes").val().length < 1){
					jQuery( "#mensaje2" ).dialog(); 
				}
				else { 				
					jQuery("#dialog").dialog({
						resizable: false,
						height: 200,
						width: 300,
						modal: true,
						buttons: {
							"Ok": function () {
								jQuery("#formulario1").submit();
							},
						}
					});
				}
			}
